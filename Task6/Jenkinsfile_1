pipeline {
  agent any 
  environment{
    SERVER_CREDENTIALS = credentials('gtihub-credentials')
  }
  stages{
    stage("docker-ps-command"){
      steps{
        sh 'docker ps -a'
        withCredentials([
          usernamePassword(credentials:'gtihub-credentials', usernameVariable: USER, passwordVariable: PWD)
        ]){
          echo "${USER} ${PWD}"
        }
      }
    }
  }
}
    
